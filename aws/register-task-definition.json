{
  "family": "oklibmagento1",
  "taskRoleArn": "arn:aws:iam::914935490550:role/ecsTaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::914935490550:role/ecsTaskExecutionRole",
  "networkMode": "bridge",
  "containerDefinitions": [
    {
      "name": "web",
      "image": "914935490550.dkr.ecr.eu-west-1.amazonaws.com/eks-poc/ok-magento-1:latest",
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80,
          "protocol": "tcp"
        },
        {
          "hostPort": 443,
          "containerPort": 443,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "memory": 512,
      "environment": [
        {
          "name": "MYSQL_HOST",
          "value": "db"
        },
        {
          "name": "MYSQL_DATABASE",
          "value": "magento"
        },
        {
          "name": "MYSQL_USER",
          "value": "magento"
        },
        {
          "name": "MYSQL_PASSWORD",
          "value": "magento"
        },
        {
          "name": "MAGENTO_LOCALE",
          "value": "en_GB"
        },
        {
          "name": "MAGENTO_TIMEZONE",
          "value": "Europe/Amsterdam"
        },
        {
          "name": "MAGENTO_DEFAULT_CURRENCY",
          "value": "EUR"
        },
        {
          "name": "MAGENTO_ADMIN_FIRSTNAME",
          "value": "Mage"
        },
        {
          "name": "MAGENTO_ADMIN_LASTNAME",
          "value": "Admin"
        },
        {
          "name": "MAGENTO_ADMIN_EMAIL",
          "value": "admin@exmaple.org"
        },
        {
          "name": "MAGENTO_ADMIN_USERNAME",
          "value": "admin"
        },
        {
          "name": "MAGENTO_ADMIN_PASSWORD",
          "value": "magentorocks1"
        },
        {
          "name": "MAGENTO_URL",
          "value": "http://oklibmagento1.okit.io"
        },
        {
          "name": "OKENV",
          "value": "beta"
        },
        {
          "name": "OKCASHSECRET",
          "value": "sk_0af984d1-1377-4e82-b36d-d66aae632c46"
        },
        {
          "name": "OKOPENSECRET",
          "value": "sk_cf1b3e53-dd3a-4df3-b9a7-388c92b4a301"
        }
      ],
      "mountPoints": [],
      "volumesFrom": [],
      "dnsServers": [],
      "links": [
        "db:db"
      ]
    },
    {
      "name": "db",
      "image": "mysql:5.6.23",
      "portMappings": [],
      "essential": true,
      "memory": 512,
      "environment": [
        {
          "name": "MYSQL_ROOT_PASSWORD",
          "value": "myrootpassword"
        },
        {
          "name": "MYSQL_DATABASE",
          "value": "magento"
        },
        {
          "name": "MYSQL_USER",
          "value": "magento"
        },
        {
          "name": "MYSQL_PASSWORD",
          "value": "magento"
        }
      ],
      "mountPoints": [
        {
          "containerPath": "/var/lib/mysql/data",
          "sourceVolume": "db-data"
        }
      ],
      "volumesFrom": [],
      "dnsServers": []
    }

  ],
  "volumes": [
    {
      "name": "db-data"
    }
  ],
  "placementConstraints": [],
  "requiresCompatibilities": [
    "EC2"
  ]
}
